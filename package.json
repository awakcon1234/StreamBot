{
  "name": "streambot",
  "version": "1.2.0",
  "description": "A self bot to stream movies/videos to Discord.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "bun src/index.ts",
    "start:node": "node dist/index.js",
    "prod:node": "set \"DOTENV_CONFIG_PATH=.env.prod\" && node -r dotenv/config dist/index.js",
    "server": "bun src/server.ts",
    "server:node": "node dist/server.js",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "author": "ysdragon",
  "license": "MIT",
  "dependencies": {
    "@dank074/discord-video-stream": "5.0.1",
    "axios": "^1.13.4",
    "bcrypt": "^6.0.0",
    "bun": "^1.3.7",
    "discord.js-selfbot-v13": "^3.7.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-session": "^1.19.0",
    "fluent-ffmpeg": "^2.1.3",
    "got": "^14.6.6",
    "multer": "^2.0.2",
    "p-cancelable": "^4.0.1",
    "play-dl": "^1.9.7",
    "tslib": "^2.8.1",
    "twitch-m3u8": "^1.1.5",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bun": "^1.3.7",
    "@types/express-session": "^1.18.2",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.9",
    "typescript": "^5.9.3"
  }
}
